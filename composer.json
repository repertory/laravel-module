{
    "name": "wangdong/laravel-module",
    "description": "Laravel模块化(附带打通lumen和laravel及不同版本限制)",
    "keywords": [
        "wangdong",
        "module",
        "laravel",
        "lumen",
        "laravel-module"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "wangdong",
        "email": "mail@wangdong.io"
    }],
    "require": {
        "illuminate/console": "~5.1",
        "illuminate/support": "~5.1",
        "league/flysystem": "^1.0"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "LaravelModule\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        },
        "laravel": {
            "providers": [
                "LaravelModule\\ServiceProvider"
            ]
        }
    }
}
